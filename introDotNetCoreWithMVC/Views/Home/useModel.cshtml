@model IEnumerable<introDotNetCoreWithMVC.Model.Drink>
@{
    Layout = null;
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>useModel</title>
</head>
<body>
    <ul>
        @foreach (var drink in Model)
        {
            <li>@drink.Name ,@drink.Price </li>

        }
    </ul>
</body>
</html>
